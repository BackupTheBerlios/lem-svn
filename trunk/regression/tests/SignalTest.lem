model TestModel is
	domain TestDomain is

		subsystem TestSubsystem is
			class A is
				event ev1 "blah";
				event ev2 "blah blah" carries (first is numeric, second is numeric);

				// empty
				behaviour is
					state sigTest is {
						objref obj;
						obj := create A;
